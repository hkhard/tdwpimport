# Controller Environment Configuration
NODE_ENV=development
PORT=3000

# Database
DATABASE_PATH=./data/tournaments.db

# JWT Secret (generate with: openssl rand -base64 32)
JWT_SECRET=your-secret-key-here

# CORS
CORS_ORIGIN=*

# WebSocket
WS_PORT=3001

# Replication (optional for standby controller)
IS_STANDBY=false
PRIMARY_CONTROLLER_URL=http://localhost:3000
REPLICATION_POLL_INTERVAL=1000
